SELECT * FROM veiculo;

-----------------------------------------------
SELECT c.nome_cliente,c.sobrenome_cliente,c.cpf_cliente, v.tipo_veiculo, v.fabricante_veiculo, v.modelo_veiculo,v.placa_veiculo
FROM cliente AS c
JOIN veiculo AS v ON c.codcliente = v.cliente_codcliente;

